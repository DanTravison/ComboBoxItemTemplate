<?xml version="1.0" encoding="utf-8" ?>
<views:ComboBoxItemView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                        xmlns:views="clr-namespace:ComboBoxItemTemplate.Views"
                        xmlns:viewmodels="clr-namespace:ComboBoxItemTemplate.ViewModels"
                        xmlns:objectmodel="clr-namespace:ObjectModel"
                        x:Class="ComboBoxItemTemplate.Views.OneLineItemView"
                        x:DataType="objectmodel:Command"
                        Margin="0">
    <Label Text="{Binding Text}" 
           Style="{StaticResource ComboBoxItemLabel}"/>
</views:ComboBoxItemView>
